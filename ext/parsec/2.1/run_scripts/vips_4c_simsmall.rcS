#!/bin/sh

# File to run the vips benchmark

export IM_CONCURRENCY=4
cd /parsec/install/bin
/sbin/m5 switchcpu
/sbin/m5 dumpstats
/sbin/m5 resetstats
./vips im_benchmark /parsec/install/inputs/vips/pomegranate_1600x1200.v /parsec/install/inputs/vips/output.v
echo "Done :D"
/sbin/m5 exit
/sbin/m5 exit
